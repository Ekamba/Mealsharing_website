{
  "name": "meal-sharing",
  "version": "1.0.0",
  "description": "Website for sharing meals",
  "engines": {
    "node": "8.11.1"
  },
  "scripts": {
    "minimist": "1.2.5",
    "start": "npm run build && node ./src/backend/index",
    "expose": "ngrok http 3000",
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon ./src/backend/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "reinstall": "npm run delete-nm & rm ./package-lock.json & npm run delete-nm & npm install",
    "delete-nm": "rm -rf node_modules/",
    "delete-pl": "rm ./package-lock.json"
  },
  "author": "Axel Ekamba",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@kodnificent/sparouter": "^1.1.0",
    "@types/xml": "^1.0.4",
    "app-root-path": "^2.2.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "disqus-react": "^1.0.8",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-form-data": "^2.0.12",
    "font-awesome": "^4.7.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "path": "^0.12.7",
    "serve-favicon": "^2.5.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^2.0.2",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "exports-loader": "^0.7.0",
    "font-awesome-loader": "^1.0.2",
    "html-webpack-plugin": "^3.2.0",
    "ngrok": "^3.2.7",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "sequelize-cli": "^5.5.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
